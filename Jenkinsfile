// jenkins pipeline script
// pipeline with stages compile, test, build with echo statements
pipeline {
    agent any
    stages {  
        stage('Compile') {
            steps {
                echo 'Compiling to check for trigger'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
    }
}

